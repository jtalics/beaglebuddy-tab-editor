package com.beaglebuddy.tab.model.attribute.note;




/**
 * This class represents a beaglebuddy tab hammer on note attribute.
 */
public class HammerOn extends NoteAttribute
{
   /**
    * default constructor.
    */
   public HammerOn()
   {
      super(NoteAttribute.Type.HammerOn);
   }

   /**
    * @return a new copy of the hammer on note attribute.
    */
   @Override
   public NoteAttribute clone()
   {
      return new HammerOn();
   }
}
