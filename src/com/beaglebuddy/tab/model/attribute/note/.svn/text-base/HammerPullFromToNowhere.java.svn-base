package com.beaglebuddy.tab.model.attribute.note;




/**
 * This class represents a beaglebuddy tab hammer on\pull off to nowhere note attribute.
 */
public class HammerPullFromToNowhere extends NoteAttribute
{
   /**
    * default constructor.
    */
   public HammerPullFromToNowhere()
   {
      super(NoteAttribute.Type.HammerPullFromToNowhere);
   }

   /**
    * @return a new copy of the hammer on\pull off to nowhere note attribute.
    */
   @Override
   public NoteAttribute clone()
   {
      return new HammerPullFromToNowhere();
   }
}
