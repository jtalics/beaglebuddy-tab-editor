package com.beaglebuddy.tab.model.attribute.chord;




/**
 * This class represents a staccato chord attribute.
 */
public class Staccato extends ChordAttribute
{
   /**
    * default constructor.
    */
   public Staccato()
   {
      super(ChordAttribute.Type.Staccato);
   }

   /**
    * @return a new copy of the staccato chord attribute.
    */
   @Override
   public ChordAttribute clone()
   {
      return new Staccato();
   }
}
